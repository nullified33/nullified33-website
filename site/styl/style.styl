$blue-700 =#1d4ed8
/* Links */
a:link
  color #1d4ed8 !important
  background-color transparent
  text-decoration none
a:visited
  color #1d4ed8 !important
  background-color transparent
  text-decoration none
a:hover
  color #1d4ed8 !important
  background-color transparent
  text-decoration underline
a:active
  color #1d4ed8 !important
  background-color transparent
  text-decoration underline

.nearest-neighbor
  image-rendering pixelated
/* End */
hr
  height 4px !important
li a.text-white
  color white !important
nav div div a.text-white
  color white !important

nav div div a:hover.text-white
  color #1d4ed8 !important
  background-color white
  text-decoration underline
nav div div a:active.text-white
  color #1d4ed8 !important
  background-color white
  text-decoration underline
p
  font-kerning normal
  font-size 15px  
/* Bootstrap 4.0 Stuff Holdovers */
.jumbotron
  padding 2rem 1rem
  margin-bottom 2rem
  background-color #e9ecef
  border-radius 0.3rem
@media (min-width 576px)
  .jumbotron
    padding 4rem 2rem
.jumbotron-fluid
  padding-right 0
  padding-left 0
  border-radius 0

/* Bootstrap Additions */
  @media only screen and (max-width 768px) 
      .card
        display block !important
        width 18rem !important
        flex none !important
/* End */ 

